<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="vivomall.dao.CouponDao">
    <select id="getCouponByGoodsId" resultType="Coupon">
        select * from coupon_t, coupon where coupon_t.coupon_id = coupon.coupon_id and goods_id = #{goodsId}
    </select>
</mapper>